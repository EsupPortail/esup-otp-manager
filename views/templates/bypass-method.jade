script#bypass-method(type='text/x-template').
    <div>
    <ul>
    <li>{{messages.api.methods.bypass.available_codes}} : {{user.methods.bypass.available_code}}</li>
    <li>{{messages.api.methods.bypass.used_codes}} : {{user.methods.bypass.used_code}}</li>
    <table class="striped" v-if="user.methods.bypass.codes.length >0">
    <thead>
    <tr>
    <th data-field="code">{{messages.api.methods.bypass.generated_codes}}</th>
    </tr>
    </thead>
    <tbody class="bypass-method-codes">
    <a href="javascript:if(window.print)window.print()"><i class="material-icons md-48">print</i></a>
    <link rel="stylesheet" href="/stylesheets/style-print-bypass-method-codes.css" media="print"/>
    <tr v-for="code in user.methods.bypass.codes">
    <td>{{code}}</td>
    </tr>
    </tbody>
    </table>
    </ul>
    <div v-if="user.methods.bypass.generation_date">
        <p>{{ messages.api.methods.bypass.generation_date.replace("%DATE%", generationDateString) }}</p>
    </div>
    <div class="row" style="text-align:center;">
    <a class="waves-effect waves-light btn" @click=generate_bypass>{{messages.api.action.generate_codes}}<i class="material-icons right" aria-hidden="true">sync</i></a>
    </div>
    <div class="divider"></div>
    </div>
